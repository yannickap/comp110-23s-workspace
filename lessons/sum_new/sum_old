"""Practicing for loops"""

pets: list[str] = ["Louie", "Bo", "Bear"]

for animal in pets:
    print(f"Good boy, {animal}!")



    """return sum of all elements in xs"""
    def sum(xs: list[float]) -> float:
        sum_total = float = 0.0
        idx: int = 0
        while idx <= len(xs) - 1:
            sum_total += xs[idx]
            idx += 1
        return sum_total